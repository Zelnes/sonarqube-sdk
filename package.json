{
  "name": "sonarqube-sdk",
  "version": "0.3.0",
  "description": "Easy way to work with Sonarqube web APIs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./dist/*": null,
    "./interfaces": "./dist/interfaces/index.js"
  },
  "typesVersions": {
    "*": {
      "interfaces": [
        "dist/interfaces/index.d.ts"
      ]
    }
  },
  "type": "module",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "test": "ava"
  },
  "engines": {
    "node": ">= 12"
  },
  "keywords": [
    "sonarqube",
    "sonarqube",
    "api",
    "sonarqube",
    "sdk",
    "sonarqube",
    "web",
    "api"
  ],
  "author": {
    "name": "Adithya Sreyaj",
    "email": "adi.sreyaj@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.10.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "ava": "^4.2.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "immer": "^9.0.6",
    "lodash-es": "^4.17.21",
    "node-fetch": "^3.0.0"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--experimental-specifier-resolution=node"
    ]
  }
}
